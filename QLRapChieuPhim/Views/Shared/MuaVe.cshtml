<script>
    function showProductTheoVT(getUrl) {
        var str = '';
        $.ajax({
            type: 'GET',
            url: getUrl,
            dataType: 'json',
            success: function (data) {
                $.each(data, function (key, value) {
                    str += `
                            <div class="showtimes-row">
                                                                <p class="ng-binding">${value.xuatChieu}</p>
                                                                <div ng-repeat="bundle in date.bundles" class="showtimes-item ng-scope">
                                                                    <label class="ng-binding">
                                                                        2D - Phụ đề
                                                                    </label>
                                                                    <div class="showtimes-list-wrapper">
                                                                        <galaxy-buy-ticket ng-repeat="show in bundle.sessions" ng-model="show" ng-movie-slug="movies[currentMovie].slug">
                                                                            <a class="showtimes-list ng-binding" href="/book-ticket/the-one.html">${value.gioChieu}</a>
                                                                        </galaxy-buy-ticket>
                                                                    </div>
                                                                </div>
                                                            </div>
                        `
                });
                $('#disKhungGio').html(str);
            },
            Error: function (xhr) {
                alert(xhr.responseText)
            }
        });
    }
</script>


