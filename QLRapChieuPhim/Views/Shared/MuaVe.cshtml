                                                        <div ng-repeat="bundle in date.bundles" class="showtimes-item"><label>{{bundle.version | uppercase | translate}} -&nbsp
 {{bundle.caption,select,
   sub {Phụ đề}
   other {L.Tiếng}
}}</label>
                                                            <div class="showtimes-list-wrapper">
                                                                <galaxy-buy-ticket ng-repeat="show in bundle.sessions" ng-model="show" ng-movie-slug="movies[currentMovie].slug"></galaxy-buy-ticket>
                                                            </div>
                                                        </div>
                                                    </div>
                                                </li>
                                            </ul>
                                        </div>
                                    </div>
                                </div>
