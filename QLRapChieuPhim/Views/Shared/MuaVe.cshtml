                                    <div class="col-md-4">
                                        <div class="panel panel-default">
                                            <div class="panel-heading">
                                                <h4 class="panel-title">Chọn rạp</h4>
                                            </div>
                                            <galaxy-loading ng-show="!items.cinemas"></galaxy-loading>
                                            <ul class="list-group">
                                                <li ng-repeat="cinema in items.cinemas |orderBy:&quot;km&quot;" ng-click="getMovieByCinema(cinema,$index)">
                                                    <a href="javascript:;" ng-class="{&quot;active&quot;:currentCinema==$index}" class="list-group-item">
                                                        <div class="showtimes-row">
                                                            <div class="title-movie">
                                                                <p>{{cinema.name}}</p><span ng-if="cinema.km" class="distance">{{cinema.km || '-|-' | number : 2}} km</span></div>
                                                        </div>
                                                    </a>
                                                </li>
                                            </ul>
                                        </div>
                                    </div>
                                    <div class="col-md-4">
                                        <div class="panel panel-default">
                                            <div class="panel-heading">
                                                <h4 class="panel-title">Chọn phim</h4>
                                            </div>
                                            <galaxy-loading ng-show="onLoadItem"></galaxy-loading>
                                            <ul class="list-group">
                                                <li ng-if="movies.length == 0 &amp;&amp; !onLoadItem" class="list-group-item-text"><span ng-if="currentMovie == -1">Vui lòng chọn phim</span><span ng-if="currentMovie != -1">Hiện chưa có lịch chiếu</span></li>
                                                <li ng-repeat="movie in movies" ng-click="setDates(movie.dates,$index,&quot;movie&quot;)">
                                                    <a href="javascript:;" ng-class="{&quot;active&quot;:currentMovie==$index}" class="list-group-item movie">
                                                        <div class="showtimes-row"><img src="https://cdn.galaxycine.vn{{movie.imageLandscape}}"><i class="{{getClassIcon(movie.age)}}"></i>
                                                            <div class="title-movie">
                                                                <p class="upper-text">{{movie.name}}</p>
                                                                <!--p.vn 5 anh em siêu nhân-->
                                                            </div>
                                                        </div>
                                                    </a>
                                                </li>
                                            </ul>
                                        </div>
                                    </div>
                                    <div class="col-md-4">
                                        <div class="panel panel-default">
                                            <div class="panel-heading">
                                                <h4 class="panel-title">Chọn suất</h4>
                                            </div>
                                            <ul class="list-group">
                                                <li ng-if="dates.length == 0 " class="list-group-item-text">Vui lòng chọn suất</li>
                                                <li ng-repeat="date in dates" class="list-group-item">
                                                    <div class="showtimes-row">
